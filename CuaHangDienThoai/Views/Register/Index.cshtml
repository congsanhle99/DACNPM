<body class="sign-in-up">
    <section>
        <div id="page-wrapper" class="sign-in-wrapper">
            <div class="graphs">
                <div class="sign-up">
                    <h3>Đăng ký</h3>
                    <p class="creating">
                    </p>
                    <form action="/Register/Register" method="post" id="formRegister">
                        <h5>Thông tin đăng ký</h5>
                        <div class="sign-u">
                            <div class="sign-up1">
                                <h4>Tài khoản* :</h4>
                            </div>
                            <div class="sign-up2">
                                <input type="text" placeholder=" " required=" " name="username" />
                            </div>
                            <div class="clearfix"> </div>
                        </div>
                        <div class="sign-u">
                            <div class="sign-up1">
                                <h4>Địa chỉ:</h4>
                            </div>
                            <div class="sign-up2">
                                <input type="text" placeholder=" " required=" " name="address" />

                            </div>
                            <div class="clearfix"> </div>
                        </div>
                        <div class="sign-u">
                            <div class="sign-up1">
                                <h4>Email* :</h4>
                            </div>
                            <div class="sign-up2">
                                <input type="text" placeholder=" " required=" " name="email" />
                            </div>
                            <div class="clearfix"> </div>
                        </div>
                        <h6>Thông tin đăng nhập</h6>
                        <div class="sign-u">
                            <div class="sign-up1">
                                <h4>Password* :</h4>
                            </div>
                            <div class="sign-up2">
                                <input type="password" placeholder=" " required=" " name="password" />
                            </div>
                            <div class="clearfix"> </div>
                        </div>
                        <div class="sign-u">
                            <div class="sign-up1">
                                <h4>Nhập lại password* :</h4>
                            </div>
                            <div class="sign-up2">
                                <span style="display:none; color:red" id="msg">Mật khẩu không khớp</span>
                                <input type="password" placeholder=" " required=" " name="rePassword" />
                            </div>
                            <div class="clearfix"> </div>
                        </div>
                        <!--Captcha-->
                        <div class="g-recaptcha" hl="" data-sitekey="6LfPra0ZAAAAAF5jnO5bRFRUm18QBxCBywV_iA3D"></div>
                        <div class="sub_home">
                            <div class="sub_home_left">
                                <input type="submit" value="Đăng ký">
                            </div>
                            <div class="sub_home_right">
                                <p>Quay lại <a href="index.html">Trang chủ</a></p>
                            </div>
                            <div class="clearfix"> </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>

    <script src="~/Public/admin2/js/jquery.nicescroll.js"></script>
    <script src="~/Public/admin2/js/scripts.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Public/admin2/js/bootstrap.min.js"></script>

    <script>
        $(document).ready(function () {
            $('#formRegister').submit(function (e) {
                if ($('[name="password"]').val() != $('[name="rePassword"]').val()) {
                    e.preventDefault();
                    $('#msg').show();
                }
            })
        })
    </script>
</body>
</html>